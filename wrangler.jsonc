// For use with Cloudflare Workers. This is necessary for a “Pages”-like static
// website.
//
// You MUST change “name” to your Cloudflare Workers project name.
//     should change “compatibility_date” to the date last deployed.
{
  “$schema”: “node_modules/wrangler/config-schema.json”,
  “name”: “website”,
  “compatibility_date”: “2025-08-27”,
  “preview_urls”: false,
  “assets”: {
    “directory”: “./content”
  }
}
